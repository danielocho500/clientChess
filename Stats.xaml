<Window x:Class="Cliente.Stats"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cliente"
        mc:Ignorable="d"
        Title="Stats" Height="450" Width="800">
    <Grid>
        <Label x:Name="lbMatchesPlayed" Content="Matches played:" Margin="44,26,593,332" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.694"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.655"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="lbWinP" Content="Win %:" Margin="44,101,581,265"></Label>
        <Label x:Name="lbEloM" Content="Elo_max:" Margin="44,159,563,193"></Label>
        <Label x:Name="lbEloA" Content="Elo actual:" Margin="44,237,499,129"></Label>
        <Button x:Name="btnExit" Margin="397,309,345,81" Content="Exit" Click="btnExit_Click"></Button>
    </Grid>
</Window>
