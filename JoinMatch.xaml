<Window x:Class="Cliente.JoinMatch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cliente"
        mc:Ignorable="d"
        Title="JoinMatch" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TBCode" HorizontalAlignment="Left" Height="23" Margin="222,114,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Content="Code" HorizontalAlignment="Left" Margin="138,114,0,0" VerticalAlignment="Top"/>
        <Button x:Name="BtnJoin"  Content="Join" HorizontalAlignment="Left" Margin="200,192,0,0" VerticalAlignment="Top" Width="75" Click="BtnJoin_Click" />
        <Button x:Name="btnExit" Content="Exit" HorizontalAlignment="Left" Margin="318,201,0,0" VerticalAlignment="Top" Width="75" Click="btnExit_Click"/>

    </Grid>
</Window>
